{
  "name": "webpack_boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "sprite": "spritesmith --rc .sprite.config.js",
    "svgsprite": "svg-sprite -C .svg.common.config.json ../common/src/img/_svgs/_sprites/* && svg-sprite -C .svg.config.json src/img/_svgs/_sprites/*",
    "nodemon": "nodemon --watch webpack.dev.config.js --watch webpack.common.config.js --exec \"webpack-dev-server --config webpack.dev.config.js\"",
    "dev": "npm run clean && npm run sprite && npm run svgsprite && npm run nodemon",
    "build": "npm run clean && npm run sprite && webpack --config webpack.prod.config.js --env.prod"
  },
